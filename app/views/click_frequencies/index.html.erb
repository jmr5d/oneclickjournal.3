<h1>Listing click_frequencies</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Click count</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @click_frequencies.each do |click_frequency| %>
      <tr>
        <td><%= click_frequency.user %></td>
        <td><%= click_frequency.click_count %></td>
        <td><%= link_to 'Show', click_frequency %></td>
        <td><%= link_to 'Edit', edit_click_frequency_path(click_frequency) %></td>
        <td><%= link_to 'Destroy', click_frequency, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Click frequency', new_click_frequency_path %>
